{
  "name": "@akatek/timeslot",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "keywords": [
    "timeslot",
    "timeslots"
  ],
  "author": "Arthur Jacquemin <arthurjacquemin2>",
  "scripts": {
    "build": "esbuild src/index.ts --bundle --outfile=dist/index.js --format=cjs",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "lint:fix": "eslint --fix --ignore-path .eslintignore --ext .js,.ts .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\""
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vitest/coverage-istanbul": "^0.28.2",
    "esbuild": "^0.17.4",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "prettier": "^2.8.3",
    "typescript": "^4.9.4",
    "vitest": "^0.28.2"
  },
  "dependencies": {
    "time-slotter": "^1.0.10"
  }
}
